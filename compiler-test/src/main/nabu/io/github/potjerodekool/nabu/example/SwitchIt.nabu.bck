package io.github.potjerodekool.nabu.example;

fun intSwitch(value: int): boolean {
    switch(value) {
        case 6, 7, 8 : {
            return true;
        }
        case 9 : {
            return true;
        }
        case 10 : {
            return true;
        }
        default :{
            return false;
        }
    }
}

