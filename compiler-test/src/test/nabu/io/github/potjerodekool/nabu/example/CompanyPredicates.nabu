package io.github.potjerodekool.nabu.example;

import io.github.potjerodekool.nabu.lang.jpa.JpaPredicate;
import io.github.potjerodekool.nabu.lang.jpa.InnerJoin;
import io.github.potjerodekool.employee.Company;

fun findCompanyByEmployeeFirstName(employeeFirstName: String): JpaPredicate<Company> {
    return { c : Company ->
        var e = c.employees as InnerJoin;
        e.firstName == employeeFirstName;
    }
}
